    "@solana/wallet-adapter-react-ui": "0.9.6",
    "@solana/wallet-adapter-wallets": "0.16.1",
    "@solana/web3.js": "1.43.2",
    "concurrently": "7.2.1",
    "javascript-time-ago": "2.3.13",
    "next": "12.1.6",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-hot-toast": "2.2.0",
    "react-icons": "4.3.1",
    "react-simple-hook-modal": "1.1.0",
    "react-spinners": "0.12.0",
